"pactl set-sink-mute 0 false ; pactl set-sink-volume 0 +5% ; pkill -RTMIN+10 i3blocks"
  XF86AudioRaiseVolume

"pactl set-sink-mute 0 false ; pactl set-sink-volume 0 -5% ; pkill -RTMIN+10 i3blocks"
  XF86AudioLowerVolume

"pactl set-sink-mute 0 toggle ; pkill -RTMIN+10 i3blocks"
  XF86AudioMute

"pactl set-source-mute 1 toggle ; pkill -RTMIN+10 i3blocks"
  XF86AudioMicMute

"xbacklight -inc 10"
  XF86MonBrightnessUp

"xbacklight -dec 10"
  XF86MonBrightnessDown
  
"mpc toggle"
  XF86AudioPlay

"mpc stop"
  XF86AudioStop

"mpc prev"
  XF86AudioPrev

"mpc next"
  XF86AudioNext

"/opt/lock"
  XF86ScreenSaver
